<p style="color: green"><%= notice %></p>

<% content_for :title, "Articulos" %>

<h1>Articulos</h1>

<div id="articulos">
  <% @articulos.each do |articulo| %>
    <%= render articulo %>
    <p>
      <%= link_to "Show this articulo", articulo %>
    </p>
  <% end %>
</div>

<%= link_to "New articulo", new_articulo_path %>
